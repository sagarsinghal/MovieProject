 CREATE TABLE MOVIE (MOVIE_ID NUMBER(10) PRIMARY KEY, 
   MOVIE_NAME VARCHAR2(30), 
   TRAILER_LINK VARCHAR2(256), 
   IMAGE_LINK VARCHAR2(256), 
   PAGE_LINK VARCHAR2(256), 
   DIRECTOR VARCHAR2(30), 
   PRODUCER VARCHAR2(30), 
   CASTS VARCHAR2(100), 
   RELEASE_DATE VARCHAR2(10), 
   DES VARCHAR2(100),
   RATING VARCHAR2(10));   
